class AuthGuardService {
    constructor(tokenServe, router) {
        this.tokenServe = tokenServe;
        this.router = router;
    }
    canActivate(route, state) {
        let url = state.url;
        if (this.tokenServe.getToken() !== null) {
            console.log('router success!');
            return true;
        }
        this.router.navigate(['/login']);
        return false;
    }
}
