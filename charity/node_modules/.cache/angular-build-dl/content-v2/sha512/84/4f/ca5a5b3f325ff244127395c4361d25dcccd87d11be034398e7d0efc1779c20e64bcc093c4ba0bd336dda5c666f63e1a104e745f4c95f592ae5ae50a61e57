/*! exports provided: passwordEquals */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "passwordEquals", function() { return passwordEquals; });
//判断密码和重复密码输入是否相等,如果相等返回null，如果不相等返回control.value
function passwordEquals(password) {
    return (control) => {
        const isEquals = (password === control.value);
        return isEquals ? null : { 'comparePassword': control.value };
    };
}


